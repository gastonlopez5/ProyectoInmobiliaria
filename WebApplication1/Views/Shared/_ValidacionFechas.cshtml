<script type="text/JavaScript">
    function validar() {
            var fechaInicio, fechaFin, fechaActual, hoy, dia, mes;

            hoy = new Date();
            dia = hoy.getDay().toString();
            if (dia.length <= 1) {
                dia = "0" + dia;
            }
            mes = (hoy.getMonth() + 1).toString();
            if (mes.length <= 1) {
                mes = "0" + mes;
            }

            fechaActual = hoy.getFullYear() + "-" + mes + "-" + dia;
            fechaIncio = document.getElementById("FechaInicio").value;
            fechaFin = document.getElementById("FechaFin").value;

            if (fechaInicio < fechaActual || fechaFin < fechaActual) {
                alert("Alguna de las fechas ingresadas es anterior a la fecha actual");
                return false;
            }

            return false;

        }
</script>
